<?xml version="1.0"?>
<package >
  <metadata>
    <id>SqlTest</id>
    <version>$version$</version>
    <title>SqlTest</title>
    <authors>@wojo</authors>
    <owners>@wojo</owners>
    <licenseUrl>https://github.com/MarkWojciechowicz/SqlTest/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/MarkWojciechowicz/SqlTest</projectUrl>
    <iconUrl>https://github.com/MarkWojciechowicz/SqlTest/tree/master/SqlTest/Content/SqlText.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Enabler library for sql and ssis unit testing</description>
    <releaseNotes>Fixed defect in which fake view was checking the wrong collection for existence.  Added nuget icon.  Added more test coverage.
    </releaseNotes>
    <copyright>Copyright 2017</copyright>
    <dependencies>
      <dependency id="nunit" version="3.7.1"/>
      <dependency id="NUnit3TestAdapter" version="3.7.0"/>
    </dependencies>
    <frameworkAssemblies>
        <frameworkAssembly assemblyName="System.Transactions"  />
    </frameworkAssemblies>
  </metadata>
  <files>
      <file src="**\bin\Release\*.dll" target="lib\net40\" />
	  <file src="content\*" exclude="**\*.pp" target="content" />
	  <file src="content\*.pp" target="Content\Samples" />
  </files>
</package>