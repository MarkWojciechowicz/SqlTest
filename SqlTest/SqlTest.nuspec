<?xml version="1.0"?>
<package >
  <metadata>
    <id>SqlTest</id>
    <version>$version$</version>
    <title>SqlTest</title>
    <authors>@wojo</authors>
    <owners>@wojo</owners>
    <licenseUrl>https://github.com/MarkWojciechowicz/SqlTest/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/MarkWojciechowicz/SqlTest/wiki</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/MarkWojciechowicz/SqlTest/master/SqlTest/SqlTest.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Sql and ssis unit testing enabler</description>
    <releaseNotes>Updated package execution to exceed 30s without throwing a timeout error
    </releaseNotes>
    <copyright>Copyright 2017</copyright>
    <dependencies>
      <dependency id="nunit" version="3.9.0"/>
      <dependency id="NUnit3TestAdapter" version="3.9.0"/>
    </dependencies>
    <frameworkAssemblies>
        <frameworkAssembly assemblyName="System.Transactions"  />
    </frameworkAssemblies>
  </metadata>
  <files>
      <file src="**\bin\Release\*.dll" target="lib\net40\" />
	  <file src="content\*" exclude="**\*.pp" target="content" />
	  <file src="content\*.pp" target="Content\Samples" />
  </files>
</package>