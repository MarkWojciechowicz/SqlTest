<?xml version="1.0"?>
<package >
  <metadata>
    <id>SqlTest</id>
    <version>$version$</version>
    <title>SqlTest</title>
    <authors>@wojo</authors>
    <owners>@wojo</owners>
    <licenseUrl>https://github.com/MarkWojciechowicz/SqlTest/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/MarkWojciechowicz/SqlTest</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Enabler library for sql and ssis unit testing</description>
    <releaseNotes>Includes app.config</releaseNotes>
    <copyright>Copyright 2017</copyright>
    <dependencies>
      <dependency id="nunit" version="3.7.1"/>
    </dependencies>
  </metadata>
  <files>
      <file src="**\bin\Release\*.dll" target="lib\net40\" />
	  <file src="content\*" exclude="*.pp" target="content" />
